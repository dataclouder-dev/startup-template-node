{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@dataclouder/nest-mongo": "<rootDir>/../libs/nest-mongo/src",
    "@dataclouder/nest-mongo/(.*)": "<rootDir>/../libs/nest-mongo/src/$1"
  }
}