{
    "name": "@dataclouder/nest-google-cloud",
    "version": "0.1.15",
    "description": "NestJS library for google cloud storage and other google cloud services",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.lib.json",
        "clean": "rm -rf dist",
        "publish:npm": "npm run clean && npm run build && npm version patch && npm publish"
    },
    "keywords": [
        "nestjs",
        "library",
        "google cloud",
        "google cloud storage"
    ],
    "author": "dataclouder",
    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": ">=10.0.0",
        "@nestjs/core": ">=10.0.0",
        "@google-cloud/storage": ">=7.15.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
