{
  "name": "scaffolddataclouder-node",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "update:dc": "npm install @dataclouder/nest-core@latest @dataclouder/nest-lessons@latest @dataclouder/nest-agent-cards@latest @dataclouder/nest-mongo@latest @dataclouder/nest-storage@latest @dataclouder/nest-whisper@latest @dataclouder/nest-vertex@latest @dataclouder/nest-users@latest @dataclouder/nest-auth@latest --force",
    "remove:mongo": "rm -rf dist/libs/nest-mongo && echo 'removed dist/libs/nest-mongo'",
    "patch:mongo": "cd libs/nest-mongo && npm version patch && echo 'patched nest-mongo'",
    "build:mongo": "cd libs/nest-mongo && npm run build && npm version patch && cp package.json ../../dist/libs/nest-mongo/ && echo 'built nest-mongo'",
    "publish:mongo": "npm run build:mongo && cd ./dist/libs/nest-mongo && npm publish && echo 'published nest-mongo'",
    "build-publish:mongo": "npm run remove:mongo && npm run patch:mongo && npm run build:mongo && npm publish:mongo",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@dataclouder/nest-agent-cards": "^0.1.35",
    "@dataclouder/nest-auth": "^0.0.10",
    "@dataclouder/nest-core": "^0.0.17",
    "@dataclouder/nest-knowledge": "^0.0.1",
    "@dataclouder/nest-lessons": "^0.1.27",
    "@dataclouder/nest-mongo": "^0.1.36",
    "@dataclouder/nest-storage": "^0.0.5",
    "@dataclouder/nest-users": "^0.0.6",
    "@dataclouder/nest-vertex": "^0.0.32",
    "@dataclouder/nest-whisper": "^0.0.6",
    "@dataclouder/notion": "^0.0.4",
    "@fastify/multipart": "^9.2.1",
    "@fastify/static": "^8.2.0",
    "@google-cloud/text-to-speech": "^5.8.0",
    "@google/genai": "^1.19.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@stable-canvas/comfyui-client": "^1.4.3",
    "@types/node-notifier": "^8.0.5",
    "@webundsoehne/nest-fastify-file-upload": "^2.3.1",
    "ai": "^4.3.16",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "firebase-admin": "^13.0.2",
    "google-auth-library": "^9.15.1",
    "groq-sdk": "^0.15.0",
    "ioredis": "^5.6.1",
    "minio": "^8.0.5",
    "mongodb": "^6.16.0",
    "mongoose": "^8.15.0",
    "node-notifier": "^10.0.1",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^4.83.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.0.8",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@dataclouder/nest-mongo(|/.*)$": "<rootDir>/libs/nest-mongo/src/$1"
    }
  }
}
